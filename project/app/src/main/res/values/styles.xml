<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="AgeRating">
        <item name="android:ellipsize">end</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:lines">1</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">@dimen/text_small</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="AnimeBreakdownGenre">
        <item name="android:gravity">end</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:paddingBottom">@dimen/root_padding_half</item>
        <item name="android:paddingEnd">@dimen/root_padding</item>
        <item name="android:paddingStart">@dimen/root_padding</item>
        <item name="android:paddingTop">@dimen/root_padding_half</item>
        <item name="keyTextColor">?android:attr/textColorPrimary</item>
        <item name="valueTextColor">?attr/colorAccent</item>
    </style>

    <style name="AnimeEpisodeCount">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/text_small</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="AnimePoster">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">100dp</item>
        <item name="actualImageScaleType">centerCrop</item>
        <item name="placeholderImage">@drawable/image_placeholder</item>
        <item name="viewAspectRatio">0.69</item>
    </style>

    <style name="AnimePoster.Small">
        <item name="android:layout_width">72dp</item>
    </style>

    <style name="AnimeSynopsis">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="AnimeSynopsis.Limited">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">5</item>
    </style>

    <style name="AnimeTitle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:lineSpacingExtra">-2dp</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">@dimen/text_xlarge</item>
        <item name="typeface">oswald_bold</item>
    </style>

    <style name="AnimeTitle.Small">
        <item name="android:textSize">@dimen/text_xlarge</item>
    </style>

    <style name="AnimeTitle.WithPadding">
        <item name="android:paddingBottom">@dimen/root_padding_half</item>
        <item name="android:paddingEnd">@dimen/root_padding</item>
        <item name="android:paddingStart">@dimen/root_padding</item>
        <item name="android:paddingTop">@dimen/root_padding_half</item>
    </style>

    <style name="AppBarLayout">
        <item name="android:background">?attr/colorPrimary</item>
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:id">@id/appBarLayout</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:theme">@style/AppBarTheme</item>
    </style>

    <style name="Avatar">
        <item name="actualImageScaleType">centerCrop</item>
        <item name="placeholderImage">@drawable/image_placeholder</item>
        <item name="roundedCornerRadius">4dp</item>
    </style>

    <style name="Avatar.Large">
        <item name="android:layout_height">@dimen/avatar_large</item>
        <item name="android:layout_width">@dimen/avatar_large</item>
    </style>

    <style name="Avatar.Medium">
        <item name="android:layout_height">@dimen/avatar_medium</item>
        <item name="android:layout_width">@dimen/avatar_medium</item>
    </style>

    <style name="Avatar.Small">
        <item name="android:layout_height">@dimen/avatar_small</item>
        <item name="android:layout_width">@dimen/avatar_small</item>
    </style>

    <style name="Badge">
        <item name="android:elevation" tools:ignore="NewApi">@dimen/root_padding_quarter</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">bottom|center_horizontal</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_small</item>
    </style>

    <style name="Badge.GroupMember">
        <item name="android:background">@drawable/bg_group_member_badge</item>
        <item name="android:id">@id/groupMemberBadge</item>
        <item name="typeface">open_sans_semibold</item>
    </style>

    <style name="Badge.Pro">
        <item name="android:background">@drawable/bg_pro_badge</item>
        <item name="android:id">@id/proBadge</item>
        <item name="android:text">@string/pro</item>
        <item name="android:visibility">gone</item>
        <item name="typeface">open_sans_bold</item>
    </style>

    <style name="BaseCardItem">
        <item name="android:layout_marginEnd">@dimen/root_padding_half</item>
        <item name="android:layout_marginStart">@dimen/root_padding_half</item>
        <item name="android:theme">@style/CardView</item>
    </style>

    <style name="BaseListOrGrid">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:scrollbars">vertical</item>
    </style>

    <style name="BaseResponsiveWidthCard" parent="BaseCardItem">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginEnd">0dp</item>
        <item name="android:layout_marginStart">0dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="CardHeader">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/root_padding_half</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="typeface">open_sans_bold</item>
    </style>

    <style name="CardItem" parent="BaseCardItem">
        <item name="android:foreground">?attr/selectableItemBackground</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="contentPaddingBottom">@dimen/root_padding</item>
        <item name="contentPaddingLeft">@dimen/root_padding</item>
        <item name="contentPaddingRight">@dimen/root_padding</item>
        <item name="contentPaddingTop">@dimen/root_padding</item>
    </style>

    <style name="CardItem.NoPadding">
        <item name="contentPaddingBottom">0dp</item>
        <item name="contentPaddingLeft">0dp</item>
        <item name="contentPaddingRight">0dp</item>
        <item name="contentPaddingTop">0dp</item>
    </style>

    <style name="CardItem.NoPadding.NothingSpecial">
        <item name="cardCornerRadius">0dp</item>
        <item name="cardElevation">0dp</item>
    </style>

    <style name="CardItem.NoPadding.NothingSpecial.Header">
        <item name="android:layout_marginTop">@dimen/root_padding_half</item>
        <item name="contentPaddingLeft">@dimen/root_padding</item>
        <item name="contentPaddingRight">@dimen/root_padding</item>
        <item name="contentPaddingTop">@dimen/root_padding_half</item>
    </style>

    <style name="CardItem.WithMargins">
        <item name="android:layout_marginBottom">@dimen/root_padding_half</item>
        <item name="android:layout_marginTop">@dimen/root_padding_half</item>
    </style>

    <style name="CollapsingToolbarLayout">
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:id">@id/collapsingToolbarLayout</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="contentScrim">?attr/colorPrimary</item>
        <item name="layout_scrollFlags">exitUntilCollapsed|scroll|snap</item>
    </style>

    <style name="CommentTextView">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColorLink">?android:attr/textColorPrimary</item>
    </style>

    <style name="CommentTextView.OP">

    </style>

    <style name="CommentTextView.OP.Limited">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">6</item>
    </style>

    <style name="CommentTextView.Reply">
        <item name="android:textSize">@dimen/text_small</item>
    </style>

    <style name="CommentTextView.Reply.Limited">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">4</item>
    </style>

    <style name="CoordinatorLayout">
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:id">@id/coordinatorLayout</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="Divider">
        <item name="android:background">?android:attr/listDivider</item>
        <item name="android:id">@id/divider</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="DividerCardItem" parent="CardItem">
        <item name="android:layout_marginEnd">0dp</item>
        <item name="android:layout_marginStart">0dp</item>
        <item name="cardElevation">0dp</item>
        <item name="cardCornerRadius">0dp</item>
    </style>

    <style name="DividerCardItem.LessPadding">
        <item name="contentPaddingBottom">@dimen/root_padding_half</item>
        <item name="contentPaddingLeft">@dimen/root_padding_half</item>
        <item name="contentPaddingRight">@dimen/root_padding_half</item>
        <item name="contentPaddingTop">@dimen/root_padding_half</item>
    </style>

    <style name="DrawerLayout">
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:id">@id/drawerLayout</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="EmptyCardText">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/root_padding</item>
        <item name="android:layout_marginEnd">@dimen/root_padding</item>
        <item name="android:layout_marginStart">@dimen/root_padding</item>
        <item name="android:layout_marginTop">@dimen/root_padding_half</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="FeedCardItem" parent="BaseCardItem">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="FloatingActionButton">
        <item name="android:id">@id/floatingActionButton</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_gravity">bottom|end</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/root_padding</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="GalleryGrid">
        <item name="spanCount">3</item>
    </style>

    <style name="GroupDescription">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="GroupDescription.Limited">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">5</item>
    </style>

    <style name="GroupLogo">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">88dp</item>
        <item name="actualImageScaleType">centerCrop</item>
        <item name="placeholderImage">@drawable/image_placeholder</item>
        <item name="viewAspectRatio">0.75</item>
    </style>

    <style name="GroupLogo.Small">
        <item name="android:layout_width">72dp</item>
    </style>

    <style name="GroupTitle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">@dimen/text_large</item>
        <item name="typeface">oswald_bold</item>
    </style>

    <style name="LikeTextView">
        <item name="android:background">?attr/selectableItemBackgroundBorderless</item>
        <item name="android:drawablePadding">@dimen/root_padding_half</item>
        <item name="android:drawableStart">@drawable/ic_favorite_border_18dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:id">@id/likeTextView</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:padding">@dimen/root_padding_half</item>
        <item name="android:textSize">@dimen/text_small</item>
        <item name="typeface">open_sans_light</item>
    </style>

    <style name="List" parent="BaseListOrGrid">
        <item name="layoutManager">LinearLayoutManager</item>
    </style>

    <style name="ListMessageContainer">
        <item name="android:clickable">true</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingEnd">@dimen/root_padding_double</item>
        <item name="android:paddingStart">@dimen/root_padding_double</item>
        <item name="android:paddingTop">@dimen/root_padding_quadruple</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="ListMessageSubtitle">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="typeface">open_sans_semibold</item>
    </style>

    <style name="ListMessageTitle">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">@dimen/text_large</item>
        <item name="typeface">open_sans_bold</item>
    </style>

    <style name="ListMessageTitle.Standalone">
        <item name="android:clickable">true</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_gravity">center_horizontal|top</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingBottom">@dimen/root_padding_quadruple</item>
        <item name="android:paddingEnd">@dimen/root_padding_double</item>
        <item name="android:paddingStart">@dimen/root_padding_double</item>
        <item name="android:paddingTop">@dimen/root_padding_quadruple</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="ListOrGrid" parent="BaseListOrGrid">
        <item name="layoutManager">LinearLayoutManager</item>
    </style>

    <style name="ListOrGrid.WithPadding">
        <item name="android:paddingEnd">@dimen/root_padding_half</item>
        <item name="android:paddingStart">@dimen/root_padding_half</item>
    </style>

    <style name="LoginTitle">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:letterSpacing" tools:ignore="NewApi">-0.05</item>
        <item name="android:lineSpacingExtra">-8dp</item>
        <item name="android:text">@string/that_lil_hummingbird</item>
        <item name="android:textSize">32sp</item>
        <item name="typeface">open_sans_bold</item>
    </style>

    <style name="MangaCover">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">100dp</item>
        <item name="actualImageScaleType">centerCrop</item>
        <item name="placeholderImage">@drawable/image_placeholder</item>
        <item name="viewAspectRatio">0.75</item>
    </style>

    <style name="MangaCover.Small">
        <item name="android:layout_width">72dp</item>
    </style>

    <style name="MangaSynopsis">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="MangaSynopsis.Limited">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">5</item>
    </style>

    <style name="MangaTitle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">@dimen/text_xlarge</item>
        <item name="typeface">oswald_bold</item>
    </style>

    <style name="MangaTitle.Small">
        <item name="android:textSize">@dimen/text_xlarge</item>
    </style>

    <style name="MediaGenres">
        <item name="android:ellipsize">end</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:maxLines">2</item>
        <item name="android:textSize">@dimen/text_small</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="MediaType">
        <item name="android:ellipsize">end</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:lines">1</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">@dimen/text_small</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="NavigationDrawerDividerView" parent="Divider">
        <item name="android:layout_marginBottom">@dimen/root_padding_half</item>
        <item name="android:layout_marginTop">@dimen/root_padding_half</item>
    </style>

    <style name="NavigationDrawerItemView">
        <item name="android:background">?attr/selectableItemBackground</item>
        <item name="android:drawablePadding">@dimen/root_padding</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">@dimen/navigation_drawer_item_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:lines">1</item>
        <item name="android:maxLines">1</item>
        <item name="android:minHeight">@dimen/list_item_height</item>
        <item name="android:paddingEnd">@dimen/root_padding</item>
        <item name="android:paddingStart">@dimen/root_padding</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="NavigationDrawerView">
        <item name="android:background">?attr/colorPrimaryDark</item>
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:id">@id/navigationDrawerView</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">@dimen/navigation_drawer_view_width</item>
    </style>

    <style name="ParallaxCoverImage">
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:id">@id/parallaxCoverImage</item>
        <item name="android:layout_height">@dimen/parallax_cover_image_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="actualImageScaleType">centerCrop</item>
        <item name="layout_collapseMode">parallax</item>
        <item name="overlayImage">@drawable/image_overlay_bottom</item>
        <item name="placeholderImageScaleType">centerCrop</item>
    </style>

    <style name="ResponsiveWidthCard" parent="BaseResponsiveWidthCard">
        <item name="android:layout_marginEnd">@dimen/root_padding_half</item>
        <item name="android:layout_marginStart">@dimen/root_padding_half</item>
    </style>

    <style name="ResponsiveWidthCard.WithPadding">
        <item name="contentPaddingBottom">@dimen/root_padding_half</item>
        <item name="contentPaddingLeft">@dimen/root_padding</item>
        <item name="contentPaddingRight">@dimen/root_padding</item>
        <item name="contentPaddingTop">@dimen/root_padding_half</item>
    </style>

    <style name="ResponsiveWidthCardHeader" parent="CardHeader">
        <item name="android:layout_marginBottom">@dimen/root_padding_half</item>
        <item name="android:layout_marginEnd">@dimen/root_padding</item>
        <item name="android:layout_marginStart">@dimen/root_padding</item>
        <item name="android:layout_marginTop">@dimen/root_padding_half</item>
    </style>

    <style name="ResponsiveWidthCardInternal">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="ResponsiveWidthCardItem">
        <item name="android:background">?attr/selectableItemBackground</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:minHeight">@dimen/list_item_height</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingBottom">@dimen/root_padding_half</item>
        <item name="android:paddingEnd">@dimen/root_padding</item>
        <item name="android:paddingStart">@dimen/root_padding</item>
        <item name="android:paddingTop">@dimen/root_padding_half</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">@dimen/text_large</item>
    </style>

    <style name="ResponsiveWidthCardItem.Switch">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ResponsiveWidthCardItemBody">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/text_small</item>
    </style>

    <style name="ResponsiveWidthCardItemBody.Standalone">
        <item name="android:layout_marginBottom">@dimen/root_padding_half</item>
        <item name="android:layout_marginTop">@dimen/root_padding_half</item>
        <item name="android:paddingEnd">@dimen/root_padding</item>
        <item name="android:paddingStart">@dimen/root_padding</item>
        <item name="android:textSize">@dimen/text_normal</item>
    </style>

    <style name="ResponsiveWidthCardItemHeader">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">@dimen/text_large</item>
    </style>

    <style name="ReviewBreakdownViewLabel">
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:paddingBottom">@dimen/root_padding_eighth</item>
        <item name="android:paddingTop">@dimen/root_padding_eighth</item>
        <item name="android:textSize">@dimen/text_xlarge</item>
    </style>

    <style name="ReviewBreakdownViewRating">
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:paddingBottom">@dimen/root_padding_eighth</item>
        <item name="android:paddingTop">@dimen/root_padding_eighth</item>
        <item name="android:textColor">?attr/colorAccent</item>
        <item name="android:textSize">@dimen/text_xlarge</item>
        <item name="typeface">open_sans_semibold</item>
    </style>

    <style name="ReviewHelpfulness">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/text_small</item>
    </style>

    <style name="ReviewSummary">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/root_padding_half</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="SectionHeader">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:paddingEnd">@dimen/root_padding_double</item>
        <item name="android:paddingStart">@dimen/root_padding_double</item>
        <item name="android:paddingTop">@dimen/root_padding</item>
        <item name="android:textSize">@dimen/text_xlarge</item>
        <item name="typeface">open_sans_extra_bold</item>
    </style>

    <style name="SectionHeader.WithMargins">
        <item name="android:layout_marginBottom">@dimen/root_padding_half</item>
    </style>

    <style name="Switch">
        <item name="android:background">@null</item>
        <item name="android:clickable">false</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="TabLayout">
        <item name="android:background">?attr/colorPrimary</item>
        <item name="android:elevation" tools:targetApi="lollipop">@dimen/root_padding_half</item>
        <item name="android:id">@id/tabLayout</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:theme">@style/ToolbarTheme</item>
        <item name="tabMode">scrollable</item>
    </style>

    <style name="TabLayout.Parallax">
        <item name="android:background">@null</item>
        <item name="android:elevation" tools:targetApi="lollipop">0dp</item>
    </style>

    <style name="TimeOrDate">
        <item name="android:ellipsize">end</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">@dimen/text_small</item>
    </style>

    <style name="Toolbar">
        <item name="android:background">?attr/colorPrimary</item>
        <item name="android:elevation" tools:targetApi="lollipop">@dimen/root_padding_half</item>
        <item name="android:id">@id/toolbar</item>
        <item name="android:layout_height">?attr/actionBarSize</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:minHeight">?attr/actionBarSize</item>
        <item name="android:theme">@style/ToolbarTheme</item>
    </style>

    <style name="Toolbar.LessElevation">
        <item name="android:elevation" tools:targetApi="lollipop">@dimen/root_padding_quarter</item>
    </style>

    <style name="Toolbar.Transparent">
        <item name="android:background">@color/translucent</item>
        <item name="android:elevation" tools:targetApi="lollipop">0dp</item>
    </style>

    <style name="Toolbar.Parallax">
        <item name="android:background">@null</item>
        <item name="android:elevation" tools:targetApi="lollipop">0dp</item>
        <item name="layout_collapseMode">pin</item>
    </style>

    <style name="UserBio">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="UserBio.Limited">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">5</item>
    </style>

    <style name="UserBio.WithMargins">
        <item name="android:layout_marginBottom">@dimen/root_padding</item>
        <item name="android:layout_marginEnd">@dimen/root_padding</item>
        <item name="android:layout_marginStart">@dimen/root_padding</item>
        <item name="android:layout_marginTop">@dimen/root_padding_half</item>
        <item name="android:textColorLink">?android:attr/textColorPrimary</item>
    </style>

    <style name="UserTitle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textSize">@dimen/text_large</item>
        <item name="typeface">oswald_bold</item>
    </style>

</resources>
